<?xml version="1.0"?>
<anjuta>
    <plugin name="Symbol Database" mandatory="no">
        <require group="Anjuta Plugin"
                 attribute="Location"
                 value="anjuta-symbol-db:SymbolDBPlugin"/>
    </plugin>
    <plugin name="Subversion" mandatory="no">
        <require group="Anjuta Plugin"
                 attribute="Location"
                 value="anjuta-subversion:Subversion"/>
    </plugin>
    <plugin name="Macro Plugin" mandatory="no">
        <require group="Anjuta Plugin"
                 attribute="Location"
                 value="anjuta-macro:MacroPlugin"/>
    </plugin>
</anjuta>
